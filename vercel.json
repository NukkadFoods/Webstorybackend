{
  "name": "webstory-backend",
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "GROQ_API_KEY": "@groq_api_key",
    "NEWS_API_KEY": "@news_api_key",
    "NODE_ENV": "production"
  }
}
